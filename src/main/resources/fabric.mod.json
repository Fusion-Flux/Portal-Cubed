  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/fluxtech",
    "sources": "https://github.com/FabricMC/fabric-example-mod",
    "issues": "https://github.com/Fusion-Flux/Origins-Umbrellas/issues"
  },

  "license": "MIT",
